{
  "name": "Project Tab Manager",
  "description": "__MSG_description__",
  "manifest_version": 2,
  "version": "2.1.1",
  "icons": {
    "48": "img/48.png",
    "128": "img/48.png"
  },
  "permissions": [
    "contextMenus",
    "bookmarks",
    "storage",
    "tabs"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/config.js",
      "js/util.js",
      "js/idb.js",
      "js/SessionManager.js",
      "js/BookmarkManager.js",
      "js/ProjectManager.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/19.png",
    "default_title": "Project Tab Manager",
    "default_popup": "ng-popup.html"
  },
  "options_page": "ng-layout.html#options",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+P"
      },
      "description": "Opens browser action popup window"
    }
  }
}
